{
  "name": "koa-knex-starter",
  "version": "1.0.0",
  "description": "Starter template for a web service based on Koa and Knex",
  "main": "src/main.js",
  "repository": "https://github.com/resolritter/koa_knex_starter",
  "author": "resolritter",
  "license": "MIT",
  "scripts": {
    "start": "node src/main",
    "dev": "nodemon src/main",
    "db:seed": "knex seed:run",
    "db:migrate": "knex migrate:latest",
    "db:new_migration": "knex migrate:make",
    "db:setup": "rimraf ./db/dev.sqlite3; npm run db:migrate && npm run db:seed",
    "lint": "eslint --ext=js ./src/",
    "format": "prettier --write .",
    "lint:fix": "npm run lint -- --fix",
    "fix": "npm run lint:fix && npm run format",
    "integration": "node -r ./env/integrationTest.js ./node_modules/.bin/jest --verbose",
    "integration:updateSnapshot": "npm run integration -- --updateSnapshot"
  },
  "prettier": {
    "semi": false,
    "trailingComma": "all"
  },
  "dependencies": {
    "bcrypt": "^5.0.0",
    "config": "^3.3.2",
    "dotenv": "^8.2.0",
    "jsonwebtoken": "^8.5.1",
    "kcors": "2.2.2",
    "knex": "0.21.12",
    "koa": "^2.13.0",
    "koa-bodyparser": "^4.3.0",
    "koa-helmet": "^6.0.0",
    "koa-jwt": "^4.0.0",
    "koa-logger": "^3.2.1",
    "koa-router": "^10.0.0",
    "lodash": "^4.17.20",
    "node-fetch": "^2.6.1",
    "p-event": "^4.2.0",
    "pino": "^6.7.0",
    "pino-colada": "^2.1.0",
    "port_acquisition_server": "git+https://github.com/resolritter/port_acquisition_server.git",
    "qs": "^6.9.4",
    "request": "^2.88.0",
    "request-promise": "^4.2.0",
    "slug": "^4.0.2",
    "sqlite3": "^5.0.0",
    "stoppable": "^1.1.0",
    "validator": "^13.1.17",
    "yup": "^0.31.0"
  },
  "devDependencies": {
    "eslint": "^7.14.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jest": "^24.1.3",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^3.1.4",
    "eslint-plugin-unused-imports": "^1.0.1",
    "faker": "^5.1.0",
    "jest": "^26.6.3",
    "nodemon": "^2.0.6",
    "prettier": "^2.2.0",
    "rimraf": "^3.0.2"
  }
}
