{
  "name": "koa-knex-realworld-example",
  "version": "1.0.0",
  "description": "conduit on koa with knex",
  "main": "src/bin/server",
  "repository": "git@github.com:gothinkster/koa-knex-realworld-example.git",
  "author": "Dmitrii Solovev <dimonnwc3@gmail.com>",
  "license": "ISC",
  "scripts": {
    "start": "node src/main",
    "dev": "nodemon src/main",
    "db:load": "knex seed:run",
    "db:migrate": "knex migrate:latest",
    "db:rollback": "knex migrate:rollback",
    "db:currentVersion": "knex migrate:currentVersion",
    "lint": "eslint \"**/*.js\"",
    "format": "prettier --write \"**/*.js\"",
    "test": "jest --coverage --verbose",
    "test:watch": "jest --watch"
  },
  "prettier": {
    "semi": false,
    "trailingComma": "all"
  },
  "jest": {
    "bail": true,
    "testEnvironment": "node",
    "coverageReporters": [
      "text"
    ]
  },
  "dependencies": {
    "bcrypt": "^5.0.0",
    "config": "^3.3.2",
    "dotenv": "^8.2.0",
    "jsonwebtoken": "^8.5.1",
    "kcors": "2.2.2",
    "knex": "0.21.12",
    "koa": "^2.13.0",
    "koa-bodyparser": "^4.3.0",
    "koa-helmet": "^6.0.0",
    "koa-jwt": "^4.0.0",
    "koa-logger": "^3.2.1",
    "koa-response-time": "^2.1.0",
    "koa-router": "^10.0.0",
    "koa-x-request-id": "^2.0.0",
    "lodash": "^4.17.20",
    "p-event": "^4.2.0",
    "pino": "^6.7.0",
    "pino-colada": "^2.1.0",
    "qs": "^6.9.4",
    "request": "^2.88.0",
    "request-promise": "^4.2.0",
    "slug": "^4.0.2",
    "sqlite3": "^5.0.0",
    "stoppable": "^1.1.0",
    "validator": "^13.1.17",
    "yup": "^0.31.0"
  },
  "devDependencies": {
    "eslint": "^7.14.0",
    "eslint-config-prettier": "^6.15.0",
    "eslint-config-standard": "^16.0.2",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-simple-import-sort": "^6.0.1",
    "eslint-plugin-standard": "^4.1.0",
    "eslint-plugin-unused-imports": "^1.0.1",
    "faker": "^5.1.0",
    "jest": "^26.6.3",
    "nodemon": "^2.0.6",
    "prettier": "^2.2.0"
  }
}
